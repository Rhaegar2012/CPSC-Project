   "cell_type": "code",

   "metadata": {},
   "outputs": [],
   "source": [
    "def Bin_Age(dataset,column=\"Age\"):\n",
    "    dataset.loc[(dataset[column]>=60)&(dataset[column]<70),\"Age_Binned\"]=\"Lower Bracket\"\n",
    "    dataset.loc[(dataset[column]>=70)&(dataset[column]<80),\"Age_Binned\"]=\"Lower middle Bracket\"\n",
    "    dataset.loc[(dataset[column]>=80)&(dataset[column]<90),\"Age_Binned\"]=\"Upper middle Bracket\"\n",
    "    dataset.loc[(dataset[column]>=90),\"Age_Binned\"]=\"Upper middle Bracket\"\n",
    "    dataset.loc[(dataset[column]<60),\"Age_Binned\"]=\"Middle Age\""
