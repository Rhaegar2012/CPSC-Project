    "Categories for binning \n",
    "\n",
    "60<age<70 = Lower bracket \n",
    "\n",
    "70<age<80 = Lower middle bracket \n",
    "\n",
    "80<age<90 = Upper middle bracket\n",
    "\n",
    "90<age = Upper bracket \n"
   ]
